        <dc:title>Learning Django Web Development</dc:title>
        <dc:creator opf:file-as="Jaiswal, Sanjeev" opf:role="aut">Sanjeev Jaiswal</dc:creator>
        <dc:contributor opf:file-as="calibre" opf:role="bkp">calibre (2.27.0) [http://calibre-ebook.com]</dc:contributor>
